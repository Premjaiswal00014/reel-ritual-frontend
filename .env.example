# Frontend Environment Variables
# Copy this file to .env and fill in your values for local development
# For production, configure these in Netlify dashboard

# API Configuration
# For local development (backend running on localhost:5000)
API_URL=http://localhost:5000/api

# For production (update with your actual backend URL)
# API_URL=https://your-backend-api.onrender.com/api
# API_URL=https://your-backend-api.railway.app/api

# Application Settings
# Can be 'development' or 'production'
NODE_ENV=development
